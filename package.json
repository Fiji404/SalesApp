{
    "name": "sales-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"vite\" \"npm:tailwind-dev\"",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "tailwind-dev": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css --watch",
        "tailwind:prod": "tailwindcss -o ./dist/styles.css --minify"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.7.1",
        "react-loading-skeleton": "^3.1.1"
    },
    "devDependencies": {
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "@vitejs/plugin-react": "^2.2.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.34.0",
        "eslint-config-react-app": "^7.0.1",
        "tailwindcss": "^3.2.6",
        "typescript": "^4.6.4",
        "vite": "^3.2.3"
    }
}
